<svg xmlns="http://www.w3.org/2000/svg" width="640" height="480" viewBox="0 0 640 480">
  <rect width="640" height="480" fill="#f5f5f5"/>
  <text x="320" y="240" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="#666">
    Spectrogram Unavailable
  </text>
  <g fill="none" stroke="#999" stroke-width="2">
    <!-- Horizontal lines representing frequency bands -->
    <line x1="120" y1="140" x2="520" y2="140" stroke-dasharray="5,5"/>
    <line x1="120" y1="200" x2="520" y2="200" stroke-dasharray="5,5"/>
    <line x1="120" y1="260" x2="520" y2="260" stroke-dasharray="5,5"/>
    <line x1="120" y1="320" x2="520" y2="320" stroke-dasharray="5,5"/>
    
    <!-- Vertical lines representing time -->
    <line x1="120" y1="120" x2="120" y2="340" />
    <line x1="220" y1="120" x2="220" y2="340" stroke-dasharray="5,5"/>
    <line x1="320" y1="120" x2="320" y2="340" stroke-dasharray="5,5"/>
    <line x1="420" y1="120" x2="420" y2="340" stroke-dasharray="5,5"/>
    <line x1="520" y1="120" x2="520" y2="340" />
    
    <!-- Axis labels -->
    <text x="320" y="380" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#666">
      Time
    </text>
    <text x="80" y="230" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#666" transform="rotate(-90,80,230)">
      Frequency
    </text>
  </g>
  
  <!-- Icon representing audio issue -->
  <g transform="translate(320, 180)">
    <circle cx="0" cy="0" r="40" fill="#e0e0e0" />
    <path d="M-15,-15 L15,15 M-15,15 L15,-15" stroke="#999" stroke-width="4" />
  </g>
</svg>